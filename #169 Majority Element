class Solution:
    def majorityElement(self, nums: List[int]) -> int:
        nums.sort()
        counter, current_max = 1, 1
        majority = nums[0]
        for i in range(len(nums) - 1):
            if nums[i] == nums[i + 1]:
                counter += 1
            else:
                if current_max < counter:
                    current_max = counter
                    majority = nums[i]
                counter = 1
        if current_max < counter:
                    current_max = counter
                    majority = nums[i]
        return majority
